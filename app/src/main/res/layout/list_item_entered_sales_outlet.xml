<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/enteredSalesOutletListItem_parentLinearLayout"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:gravity="center_vertical"
    android:orientation="vertical"
    android:background="@drawable/gray_accent_border_top_side_white_background">

    <TextView
        android:id="@+id/enteredSalesOutletListItem_salesOutletTitleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:textSize="24sp"
        android:textColor="@android:color/black"
        tools:text="Sales Outlet Title"/>

</LinearLayout>